generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Aluno {
  id        Int    @id @default(autoincrement())
  matricula String @unique
  cpf       String @unique
  nome      String
  serie     Serie

  @@map("alunos")
}

model profileImages {
  id             Int    @id @default(autoincrement())
  url            String @db.VarChar(255)
  alunoMatricula String @unique @db.VarChar(40)
}

enum Serie {
  F1
  F2
  F3
  F4
  F5
  F6
  F7
  F8
  F9
  M1
  M2
  M3
}
